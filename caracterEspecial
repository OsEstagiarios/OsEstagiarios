
<!-- Duas maneiras diferentes, verificar diferenÃ§as -->

select distinct
    fb.emp_codigo, 
    F.NUMERO, 
    F.NUMCARTPRO, 
    F.SERCARTPRO 
from 
    funcionarios f join fitabanco fb 
    on fb.numfunc = f.numero 
where instr(F.SERCARTPRO, 'x') > 0;



SELECT  
      f.numero, 
      F.NUMCARTPRO, 
      F.SERCARTPRO 
FROM
 	funcionarios f 
WHERE
	SERCARTPRO 
LIKE  
	'%58%'
AND F.SERCARTPRO is not  null;
